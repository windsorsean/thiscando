{
    "debug": true,
    "source": "local",
    "handlers": [
        {
            "name": "Add Config",
            "match": {
                "path": "/addconfig",
                "body": {
                    "name": "*",
                    "match": "*",
                    "function": "*"
                }
            },
            "function": "addConfig"
        },
        {
            "name": "Add Handler",
            "match": {
                "path": "/addhandler",
                "body": {
                    "code": "*",
                    "name": "*"
                }
            },
            "function": "addHandler"
        },
        {
            "name": "Hello World",
            "match": {
                "path": "/helloworld/*",
                "params": { "data":  "*" }
            },
            "function": "helloWorld",
            "vars": {
                "animals": ["dog", "cat"]
            }
        }
    ]
}